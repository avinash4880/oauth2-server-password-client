{
    "name": "spomky-labs/oauth2-server-password-client",
    "type": "library",
    "description": "Password Client for OAuth2 Server",
    "license": "MIT",
    "keywords": ["OAuth2", "Server", "Password", "Client"],
    "homepage": "https://github.com/Spomky-Labs/oauth2-server-password-client",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky-Labs/oauth2-server-password-client/contributors"
        }
    ],
    "require": {
        "spomky-labs/oauth2-server": "1.0.*@alpha",
        "spomky-labs/oauth2-interface": "1.0.*@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": { "OAuth2": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
